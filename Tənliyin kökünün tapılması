import math
a=int(input("a:"))
b=int(input("b:"))
c=int(input("c:"))
delta=b**2-4*a*c
print("delta=",delta)
if delta >=0:
   kök1=(-b + math.sqrt(delta)) / (2*a)
   kök2=(-b - math.sqrt(delta)) / (2*a)
   print("Tənliyin iki kökü var", kök1, kök2)
elif delta==0:
   kök=-b / (2*a)
   print("Köklər bərabərdir", kök)
else:
   print("Tənliyin kökü yoxdur")
